ngx_addon_name=ngx_http_sign_module
if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP
    ngx_module_name=$ngx_addon_name
    ngx_module_incs=
    ngx_module_deps="$ngx_addon_dir/ndk_set_var.h"
    ngx_module_srcs="$ngx_addon_dir/ndk_set_var.c $ngx_addon_dir/ngx_http_sign_module.c"
    ngx_module_libs=

    . auto/module
else
HTTP_MODULES="$HTTP_MODULES $ngx_addon_name"
NGX_ADDON_DEPS="$NGX_ADDON_DEPS $ngx_addon_dir/ndk_set_var.h"
NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ndk_set_var.c $ngx_addon_dir/ngx_http_sign_module.c"
fi
